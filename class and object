class one:
    def __init__(self,a,b):
        self.a=a
        self.b=b
    def add(self):
        print("The sum is ",self.a+self.b)
    def sub(self):
        print("The difference is",self.a-self.b)
    def mul(self):
        print("The product is ",self.a*self.b)
    def div(self):
        return self.a*self.b

o1=one(5,6)
o1.add()
mul_result=o1.mul()
print(mul_result)